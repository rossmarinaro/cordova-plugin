

<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
           id="cordova-plugin-toastyPlugin"
      version="1.3.5">
    <name>Device</name>
    <description>Cordova Device Plugin</description>
    <license>Apache 2.0</license>
    <keywords>cordova,device</keywords>
    <js-module src="toasty.js" name="device">
        <clobbers target="device" />
    </js-module>
    <engines>
      <engine name="cordova-android" version=">=4.0.0 <10.0.0" />
    </engines>

    <config-file target="config.xml" parent="/*">
        <feature name="Device">
            <param name="android-package" value="CDVDevice"/>
            <!-- <param name="onload" value="true" /> -->
        </feature>
        <!-- <source-file src="src/android/WhitelistPlugin.java" target-dir="src/org/apache/cordova/whitelist" /> -->
    </config-file>
</plugin>

